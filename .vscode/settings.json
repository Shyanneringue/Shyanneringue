        "contrastActiveBorder": "#009abd3e", //box around active border
        "contrastBorder": "#694DA4", //random lines and borders
        // Base Colors
        "focusBorder": "#694DA4",
        "foreground": "#c4bde0", //text right under tabs and in console
        "widget.shadow": "#694DA4",
        "selection.background": "#DECDE3",
        "descriptionForeground": "#170025",
        "errorForeground": "#BD49BF",
        // Text Colors — inside a text document, such as the welcome page.
        "textBlockQuote.background": "#3f27649e",
        "textBlockQuote.border": "#694da4a7",
        "textCodeBlock.background": "#170025f2",
        "textLink.activeForeground": "#A57EFC",
        "textLink.foreground": "#5489FC",
        "textPreformat.foreground": "#5489FC",
        "textSeparator.foreground": "#694DA4",
        // Button Control — button widgets such as Open Folder button in the Explorer of a new window.
        "button.background": "#BD49BF",
        "button.foreground": "#a57efc",
        "button.hoverBackground": "#DECDE3",
        // Dropdown Control — all dropdown widgets such as in the Integrated Terminal or the Output panel.
        "dropdown.background": "#3f2764f5",
        "dropdown.listBackground": "#bd49bfbb",
        "dropdown.border": "#170025",
        "dropdown.foreground": "#a67efcf7",
        // Input Control — the Search view or the Find/Replace dialog.
        "input.background": "#DECDE3",
