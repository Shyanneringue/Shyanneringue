{
    "liveServer.settings.port": 5502,
    "editor.tabCompletion": "on",
    "diffEditor.codeLens": true,
    "MutableAI.upsell": true,
    "workbench.colorTheme": "Wintry",
    "workbench.colorCustomizations": {
